{
  "name": "generator-ticker",
  "version": "1.1.1",
  "description": "An ES6 generator similar to setInterval that adjusts for slow receivers.",
  "repository": "joeycumines/generator-ticker",
  "keywords": [
    "es6",
    "generator",
    "setInterval",
    "async",
    "non-blocking",
    "timing control",
    "adjustable interval",
    "scheduling",
    "ticker",
    "performance",
    "throttle",
    "javascript",
    "typescript",
    "loop",
    "iteration",
    "delay",
    "utility",
    "time",
    "event loop"
  ],
  "author": "Joseph Cumines <joeycumines@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joeycumines/generator-ticker/issues"
  },
  "homepage": "https://github.com/joeycumines/generator-ticker#readme",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@swc/core": "^1.3.93",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.6",
    "@types/node": "20.8.2",
    "cross-env": "^7.0.3",
    "documentation": "^14.0.2",
    "gts": "^5.2.0",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "npmignore": "^0.3.0",
    "rimraf": "^5.0.5",
    "ts-chan": "^0.6.0",
    "typescript": "^5.2.2"
  },
  "type": "module",
  "source": "src/index.ts",
  "types": "build/src/index.d.ts",
  "main": "build/generator-ticker.cjs",
  "module": "build/generator-ticker.mjs",
  "unpkg": "build/generator-ticker.umd.js",
  "exports": {
    "types": "./build/src/index.d.ts",
    "import": "./build/generator-ticker.modern.mjs",
    "require": "./build/generator-ticker.cjs",
    "default": "./build/generator-ticker.modern.mjs"
  },
  "scripts": {
    "lint": "gts lint",
    "clean:gts": "gts clean",
    "compile": "microbundle -f modern,esm,cjs,umd && rimraf build/types && mv build/src build/types && rimraf build/src build/test && mv build/types/src build/src && mv build/types/test build/test && rimraf build/types",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "all": "npm-run-all -s build test",
    "clean": "npm-run-all -p clean:gts clean:pack clean:coverage",
    "clean:pack": "rimraf --glob generator-ticker-*.tgz",
    "clean:coverage": "rimraf coverage",
    "test": "npm-run-all -p lint jest typecheck",
    "build": "npm-run-all -s clean:gts compile",
    "jest": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "docs": "documentation readme src/index.ts --section API -q --parse-extension ts && v=\"$(sed '/^````/d' README.md)\" && printf '%s\n' \"$v\" >README.md",
    "release": "npm run -s build -s && npm t && git tag v$npm_package_version && git push && git push --tags && npm publish",
    "typecheck": "tsc",
    "prepack": "npmignore --auto"
  },
  "publishConfig": {
    "ignore": [
      "!/build",
      "/test",
      "/.editorconfig",
      "/.eslintignore",
      "/.eslintrc.json",
      "/.gitignore",
      "/.prettierrc.cjs",
      "/jest.config.cjs",
      "/tsconfig.json",
      "/.idea",
      "/.github",
      "/build/test",
      "/.prettierignore"
    ]
  }
}
